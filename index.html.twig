		<script id="event-template" type="text/x-handlebars-template">
		{% verbatim %}
			{{#each post}}
					<div class="event {{genre}} {{sector}}" data-title="{{slug}}">

						<div class="card card-wide mdl-card mdl-shadow--2dp" card-url="{{storylink}}">
							<a href="{{storylink}}" target="_blank">
								<div class="card-header mdl-card__title" style="background: url('{{image}}')">
									<div class="card-overlay">
										<h1 class="card-title mdl-card__title-text">{{{firstname}}} {{{name}}}</h1>
									</div>

								</div>
							</a>
							<div class="card-desc mdl-card__supporting-text">
								<span class="card-meta">{{company}}</span>
								{{#if sector}}
								  #{{sector}}
								{{/if}}

								{{#if linkedin}}
								<p>
									<i class="fa fa-linkedin-square" style="color: #0073b1"></i>
								  <a href="{{linkedin}}" target="_blank">Son profil LinkedIn</a>
								</p>
								{{/if}}


							</div>


							<div class="card-footer mdl-card__supporting-text">
								&nbsp;

								<a href="" target="_blank" id="card-nav-menu-{{id}}" class="nav-menu mdl-button mdl-js-button mdl-button--icon">
									<i class="material-icons">location_on</i>
								</a>

								<button id="card-share-menu-{{id}}" class="share-menu mdl-button mdl-js-button mdl-button--icon">
									<i class="material-icons">share</i>
								</button>
								<ul class="grid--event--share mdl-menu mdl-menu--top-right mdl-js-menu mdl-js-ripple-effect share-menu--content"
								data-mdl-for="card-share-menu-{{id}}">
								<a href="https://www.facebook.com/sharer.php?u={{storylink}}&display=popup&ref=plugin&src=share_button" target="_blank"><li class="mdl-menu__item">Facebook</li></a>
								<a href="https://twitter.com/intent/tweet?text=Forum des 100 – {{title}} – {{storylink}}&via=letemps" target="_blank"><li class="mdl-menu__item">Twitter</li></a>
								<a target="blank" href="https://www.linkedin.com/shareArticle?url={{storylink}}&mini=true&title={{title}}&summary={{null}}&source={{'Le Temps'}}"><li class="mdl-menu__item">Linkedin</li></a>
							</ul>

							</div>
						</div>
				</div>

			{{/each}}
		{% endverbatim %}
		</script>

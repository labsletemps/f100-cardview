		<script id="event-template" type="text/x-handlebars-template">
		{% verbatim %}
			{{#each post}}
					<div class="event {{genre}} {{sector}} {{canton}}" data-title="{{slug}}">

						<div class="card card-wide mdl-card mdl-shadow--2dp" card-url="{{storylink}}">
							<a href="{{storylink}}" target="_blank">

								<div class="mdl-card__title card-title-mobile">
									<h4 class="mdl-card__title-text">{{{title}}}</h4>
								</div>

								<div class="card-header mdl-card__title b-lazy {{{nopicture}}}" data-src="{{image}}">

									<div class="card-overlay">
										<h1 class="card-title mdl-card__title-text">{{{title}}} {{#if canton}}({{canton}}){{/if}}</h1>
										
										<!--<img src="img/cantons/ju.svg">-->
									</div>
								</div>
							</a>
							<div class="card-desc mdl-card__supporting-text">
								<span class="card-meta">

									<span class="card-meta-info">
										{{function}}{{#if company}}, {{company}}{{/if}}
									</span>


									<!--<span class="card-meta--tag">
										{{#if sector}}
										  #{{sector}}
										{{/if}}									
									</span>-->


								</span>

								<span class="card-meta">
									<span class="card-meta-info">
										{{age}}{{desc}}
									</span>
								</span>

								
							</div>


						

							<div class="mdl-card__actions mdl-card--border">

							    <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect lt-button">
							      Portrait
							    </a>
								{{#if linkedin}}
								    <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect li-button" href="{{linkedin}}" target="_blank">
								      <i class="fa fa-linkedin-square"></i> Profil
								    </a>
							    {{/if}}
								{{#if twitter}}
								    <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect tw-button" href="{{twitter}}" target="_blank">
								      <i class="fa fa-twitter-square"></i> Profil
								    </a>
							    {{/if}}

							</div>

							<!-- NEW -->
							<div class="mdl-card__menu">
							    <button class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect" id="card-share-menu-{{id}}">
							      <i class="material-icons">share</i>
							    </button>
							</div>
							<ul class="mdl-menu mdl-menu--bottom-left mdl-js-menu mdl-js-ripple-effect" for="card-share-menu-{{id}}">>
							  <li class="mdl-menu__item">Some Action</li>
							  <li class="mdl-menu__item mdl-menu__item--full-bleed-divider">Another Action</li>
							  <li disabled class="mdl-menu__item">Disabled Action</li>
							  <li class="mdl-menu__item">Yet Another Action</li>
							</ul>

						</div>
				</div>

			{{/each}}
		{% endverbatim %}
		</script>
